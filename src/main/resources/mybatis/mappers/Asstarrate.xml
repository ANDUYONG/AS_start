<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
	<mapper namespace="mapper.Asstarrate">
			
		<!-- update 문으로 -->
		<update id="insertAsstarrate" parameterType="java.util.Map">
		<![CDATA[
			update ASrespond set 
				asComment= #{textBox}, 
				star = #{resultcontainer} 
			where asno = #{asNo}	
		]]>
		</update>
	</mapper>
	